@model List<Orden>

@{
	ViewData["Title"] = "All Orders";
}
<div>
	<table class="table">
		<thead>
			<tr>
				<th scope="col">
					Id
				</th>
				<th scope="col">
					Provider Id
				</th>
				<th scope="col">
					User Id
				</th>
				<th scope="col">
					Total
				</th>			
			</tr>
		</thead>

		<tbody>

			@foreach (var order in Model)
			{
				<tr>
					<td>@order.Id</td>
					<td>@order.ProviderId</td>
					<td>@order.UserId</td>
					<td>@order.Total</td>
					<td>
						<a class="btn btn-outline-warning" asp-controller="Productos" asp-action="Edit">Edit</a>
					</td>
					<td>
						<a class="btn btn-outline-secondary" asp-controller="Productos" asp-action="Details">Details</a>
					</td>
					<td>
						<a class="btn btn-outline-danger" asp-controller="Productos" asp-action="Delete">Delete</a>
					</td>
				</tr>
			}			
		</tbody>
	</table>
</div>
